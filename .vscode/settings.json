{
	"editor.formatOnSave": true,
	"python.linting.pylintEnabled": true,
	"python.linting.enabled": true,
	"python.linting.pylintPath": ".env/bin/pylint",
	"python.linting.pylintArgs": [
		"--disable",
		"C0114",
		"--disable",
		"C0115",
		"--disable",
		"C0116",
		"--disable",
		"W0212"
	],
	"git.ignoreLimitWarning": true,
	"cSpell.words": [
		"Bluesberry",
		"Gravano",
		"Hcap",
		"Hogmanay",
		"Matowi",
		"Primitivo",
		"Sebastianna",
		"Shangjia",
		"Sisu",
		"Ulusaba",
		"astimezone",
		"dateutil",
		"fillna",
		"gettz",
		"iloc",
		"inplace",
		"inplay",
		"isabstractmethod",
		"isnan",
		"itertools",
		"numpy",
		"rmdir",
		"standardise",
		"strftime",
		"strptime",
		"tolist",
		"tzinfo",
		"unrequired",
		"waaaaaah",
		"weeeee",
		"wwwaaaahhhhhhh",
		"wwweeeeeeeeeee",
		"wwweeeeeeeeeeeeeeeeee"
	],
	"python.formatting.provider": "black",
	"python.analysis.extraPaths": [
		"./app"
	]
}
